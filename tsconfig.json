{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": "./",
    "outDir": "dist/",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "paths": {
      "catalogs/*": ["./catalogs/*"],
      "utils/*": ["./utils/*"]
    }
  }
}
